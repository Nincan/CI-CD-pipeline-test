{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "build-draft": "node resetBasePath && next build",
    "postbuild": "yarn sitemap",
    "build:examples": "tsc --project examples.tsconfig.json && pretty-quick",
    "sitemap": "node ../docs/sitemapGenerator.js",
    "start": "next start",
    "export": "next export -o build",
    "export-draft": "next export -o build",
    "publish-draft": "netlify deploy -d build",
    "publish-aws": "node upload-to-S3.js",
    "serve-public": "http-server public --silent &",
    "pa11y-ci:sitemap": "wait-on --timeout 10000 http://localhost:8080 && pa11y-ci --sitemap http://localhost:8080/sitemap.xml",
    "kill-server": "pkill -f http-server",
    "a11y-check": "npm-run-all serve-public pa11y-ci:sitemap kill-server"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.21.5",
    "@docsearch/js": "^3.0.0-alpha.40",
    "@mdx-js/tag": "^0.20.3",
    "aws-sdk": "^2.799.0",
    "classnames": "2.2.6",
    "csstype": "^3.0.2",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.15",
    "match-sorter": "^4.0.2",
    "next": "12.0.10",
    "next-server": "^9.0.5",
    "next-transpile-modules": "^9.0.0",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "^1.0.2",
    "react": "^17.0.2",
    "react-dnd": "2.5.4",
    "react-dnd-html5-backend": "2.5.4",
    "react-dom": "^17.0.2",
    "react-frame-component": "^4.1.3",
    "react-highlight-words": "^0.16.0",
    "react-icons": "^3.10.0",
    "react-json-view": "^1.19.1",
    "react-use": "^15.3.4",
    "react-view": "^2.3.7",
    "rickmortyapi": "0.2.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.2.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.4",
    "@types/react": "^17.0.39",
    "@zeit/next-mdx": "^1.2.0",
    "axios": "^0.24.0",
    "http-server": "^14.1.1",
    "mime-types": "^2.1.34",
    "netlify-cli": "~2.69.1",
    "next-images": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^3.0.1",
    "pa11y-ci-reporter-html": "^6.0.0",
    "raw-loader": "^4.0.0",
    "recursive-readdir": "^2.2.2",
    "wait-on": "^7.0.1"
  }
}
